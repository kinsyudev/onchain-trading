{
  "name": "rabbitmq-consumer",
  "module": "src/index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "pnpm with-env tsx --watch src/index.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env tsx src/index.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "devDependencies": {
    "@kinsyu/eslint-config": "workspace:*",
    "@kinsyu/prettier-config": "workspace:*",
    "@kinsyu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@kinsyu/messaging": "workspace:*",
    "@kinsyu/validators": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "dotenv-cli": "catalog:",
    "zod": "catalog:"
  },
  "prettier": "@kinsyu/prettier-config"
}
